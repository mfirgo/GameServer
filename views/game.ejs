<html>
    <meta charset="utf-8" />
    <head>
    <title>Tic Tac Toe</title>
    <link rel="stylesheet" href="style.css">  <!-- źle -->
    <script src="/socket.io/socket.io.js"></script>   <!-- źle -->
    <script src="tictactoejs.js" charset="utf-8"></script>   <!-- źle -->
    </head>
  <body>

    <div class="player p2">
      Player 1
      <div class="connected">Connected <span></span></div>
      <div class="ready">Ready <span></span></div>
    </div>
    <div class="player p1">
      Player 2
      <div class="connected">Connected <span></span></div>
      <div class="ready">Ready <span></span></div>
    </div>

    <div class="container">
        <div class="grid grid-user"></div>
    </div>

    <div class="hidden-info">
      <button id="start">Start New Game</button>
      <button id="leave">Leave Room</button>
      <h3 id="whose-move"></h3>
      <h3 id="info">Game ready to start</h3>
    </div>

</body>
</html>